Host github.com
HostName github.com 
User git 
IdentityFile ~/.ssh/GitHub_rsa

Host servalan.lan
    StrictHostKeyChecking no
    UserKnownHostsFile /dev/null
    ProxyCommand=$(sshnp -f @cconstab -t @ssh_1 -h @rv_am -d servalan -x -o "-W %h:%p")
    IdentityFile ~/.ssh/GitHub_rsa
#    LogLevel QUIET

