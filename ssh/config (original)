Host iot_device01.lan
    StrictHostKeyChecking no
    UserKnownHostsFile /dev/null
    ProxyCommand=$(sshnp -f @cconstab -t @ssh_1 -h @rv_am -d iot_device01 -x -o "-W %h:%p" 2>/dev/null)
    IdentityFile ~/.ssh/GitHub_rsa
#    LogLevel QUIET

